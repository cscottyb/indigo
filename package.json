{
  "name": "indigo",
  "version": "0.1.0",
  "description": "Duck Duck Go Indigo Theme.",
  "main": "src/js/index.js",
  "scripts": {
    "test": "node-sass -w src/build/ -o test/",
    "tests": "sass --no-source-map src/build/:test/",
    "watch-scss": "node-sass --w src/scss -o src/build/css",
    "watch-modules": "node-sass -w src/scss/components -o src/build/css/components",
    "watch-modules-dark": "node-sass -w src/scss/components-dark -o src/build/css/components-dark",
    "watch-modules-dim": "node-sass -w src/ -o test/"
  },
  "keywords": [
    "duckduckgo",
    "ddg",
    "duck",
    "indigo",
    "theme"
  ],
  "author": "Scott Borland",
  "license": "MIT",
  "dependencies": {
    "node-sass": "^5.0.0",
    "sass": "^1.39.2",
    "sass-watch": "^0.1.0"
  }
}
